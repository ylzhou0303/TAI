  RATE LIST
    EXTERNAL_FILE RateListTide_MI.txt


 
  TIME_UNITS hr
  DATA_UNITS m^3/hr   m^3/hr   MW
   0.d0   -0.0001   0.d0    0.d0
   2.d0    0.0001    0.d0   0.d0
  10.d0   -0.0001        0      0
 

 /
noskip 
/


# inject oxygen evenly, not creating heterogeneity

FLOW_CONDITION ROL_homo    #inject Oxygen via several hotspots, to create spatial heterogeneity
  TYPE
    RATE volumetric_rate
   /
  
  RATE LIST
    TIME_UNITS hr
    DATA_UNITS m^3/hr   m^3/hr   MW
      
      0.d0   0.0204d-7   0.d0    0.d0   
      # 1.d-7 divided by number of columns; 
      
  /
/

FLOW_CONDITION ROL_het    #inject Oxygen via several hotspots, to create spatial heterogeneity
  TYPE
    RATE volumetric_rate
   /
  
  RATE LIST
    TIME_UNITS hr
    DATA_UNITS m^3/hr   m^3/hr   MW
      
      0.d0   0.11111d-9   0.d0    0.d0   
      # 1.d-7 divided by number of roots; 
      #if injection from top, i.e., no heterogeneity of O2, then divided by the number of columns
  /
/



FLOW_CONDITION O2_diffusion
  TYPE
    RATE volumetric_rate
  /
  #RATE 0.0204d-7  0  0 m^3/hr m^3/hr MW

  RATE LIST
    EXTERNAL_FILE RateListO2_CB.txt
  /
  
/

#=========================== condition couplers ===============================
skip
# Tides
# Add or remove  water mass from the sediment column
# This process does not work if I have set up a boundary condition, as the boundary condition would force the water content
# to stay constant, to match the boundary condition

SOURCE_SINK tide
  FLOW_CONDITION tide
  TRANSPORT_CONDITION tide
  REGION top
END
noskip


# Initial condition

INITIAL_CONDITION     # initial condition
  TRANSPORT_CONDITION initial
  FLOW_CONDITION initial
  REGION all
END


# Boundary condition at the sediment-air-interface

BOUNDARY_CONDITION sed_air_interface
  TRANSPORT_CONDITION sed_air_interface
  FLOW_CONDITION sed_air_interface
  REGION top
END




skip
#O2 diffusion from top 
#This is to simulate the oxygen fluctuation due to water table change,
# when water table is below sediment surface, increase the O2 diffusion rate as water table draws down
#I have a list of O2 input rate wich varies with the water table over time in the FLOW CONDITION

SOURCE_SINK O2_diffusion            
  FLOW_CONDITION O2_diffusion
  TRANSPORT_CONDITION O2_diffusion
  REGION top_layer
END
noskip



skip
# Homogeneous Root Oxygen Release: Inject Oxygen evenly, i.e. no heterogeneity

SOURCE_SINK ROL_homo    #Root Oxygen Release, homogeneous           
