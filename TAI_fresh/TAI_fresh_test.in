#Description: Redox ladder ation), with ELM CTC bgc sandboxes
# Rate constants estimated to show different processes

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE GENERAL
      OPTIONS
        #ARITHMETIC_GAS_DIFFUSIVE_DENSITY
      /
    /
    SUBSURFACE_TRANSPORT transport
      MODE GIRT
      #NUMERICAL_JACOBIAN
    /
  /
END

SUBSURFACE

#=========================== useful transport parameters =======================


REFERENCE_LIQUID_DENSITY 1.d3


#+++++++++++++++++++++++ I. DOMAINS +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
#
skip    ! if skipped, CLM grids will be enforced to replace PF-mesh defined below (discretization/region) 
#======================= I-0. CLM <==> PFLOTRAN domain mapping files ==========
MAPPING_FILES                   
  !
END
noskip

#======================= I-1. discretization ===================================
# NOTE: the following ONLY provides required data initialization, and will be 
#       over-riddn by CLM domains.
GRID
  TYPE structured
  ORIGIN 0.d0 0.d0 0.d0
  NXYZ 1 1 10
  BOUNDS
    0.d0  0.d0  0.d0
    1.0d0 1.0d0 20.0d0
  /
  skip
  DXYZ 
     1.0d0 
     1.0d0 
     10.0d0\
     10.0d0
  /
  noskip
END

PROC 1 1 1

#======================= I-2.regions ===========================================
REGION top
  COORDINATES
    0.d0 0.d0 20.d0
    1.d0 1.d0 20.d0
  /
  FACE TOP
END

REGION bottom
  COORDINATES
    0.d0 0.d0 0.0d0
    1.d0 1.d0 0.0d0
  /
  FACE BOTTOM
END


REGION all
  COORDINATES
    0.d0 0.d0 0.d0
    1.d0 1.d0 20.d0
  /
  FACE TOP
END



#==============================flow condition======================================
FLOW_CONDITION tide
  TYPE
    RATE volumetric_rate
  /
  RATE  0.1  0  0 m^3/d m^3/d MW
/



FLOW_CONDITION initial
  TYPE
    GAS_PRESSURE dirichlet
    GAS_SATURATION dirichlet
    TEMPERATURE dirichlet
  /
  GAS_PRESSURE 1.d5
  GAS_SATURATION 0.5
  TEMPERATURE 20.d0
/


#=========================== transport conditions =============================

TRANSPORT_CONDITION initial
  TYPE dirichlet
  CONSTRAINT_LIST
    0.d0 initial
  /
END


TRANSPORT_CONDITION tide
  TYPE dirichlet
  CONSTRAINT_LIST
   0.d0 tide
  /
END





#=========================== output options ===================================
OUTPUT
  !PERIODIC_OBSERVATION TIMESTEP 1
  SCREEN PERIODIC 1000   #show the results on screen every 1000 timesteps
!  PERIODIC TIMESTEP 1
  FORMAT TECPLOT POINT
  PERIODIC TIME 24 h
  VARIABLES
    #TEMPERATURE
    LIQUID_SATURATION
  /
END

#=========================== times ============================================
TIME
  FINAL_TIME 24 h
  INITIAL_TIMESTEP_SIZE 0.5d0 h
  MAXIMUM_TIMESTEP_SIZE 12.d0 h
END

#=========================== regions ==========================================
REGION obs_pt
  COORDINATE 0.5 0.5 0.5
END

#=========================== observation points ===============================
OBSERVATION
  REGION obs_pt
END


#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  TRANSPORT_CONDITION initial
  FLOW_CONDITION initial
  REGION all
END




SOURCE_SINK tide
  FLOW_CONDITION tide
  TRANSPORT_CONDITION tide
  REGION all
END


